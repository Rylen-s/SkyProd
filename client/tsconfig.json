{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "strict": true,
    "types": [ "node" ],
"typeRoots": [ "../node_modules/@types" ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "client/.expo/types/**/*.ts",
    "expo-env.d.ts",
    "global.d.ts",
    ".expo/types/**/*.ts"
  ]
}
